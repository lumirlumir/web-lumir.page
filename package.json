{
  "scripts": {
    "start": "npm run sass:compile && concurrently \"tidory start\" \"npm run sass:watch\"",
    "preview": "npm run sass:compile && concurrently \"tidory preview\" \"npm run sass:watch\"",
    "production": "tidory production",
    "store": "tidory store",
    "deploy": "tidory deploy",
    "lint": "eslint --ext .js --ignore-path .gitignore . && pug-lint .",
    "sass:watch": "node-sass -w style/style.scss style/style.css --output-style expanded",
    "sass:compile": "node-sass style/style.scss style/style.css --output-style expanded"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tidory/tidory"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.8",
    "eslint": "^8.22.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-webpack-plugin": "^3.2.0",
    "concurrently": "^8.2.0",
    "node-sass": "^8.0.0",
    "pug-lint": "^2.6.0"
  },
  "dependencies": {
    "alpinejs": "^3.11.1",
    "tailwindcss": "^3.2.4",
    "tidory": "^8.3.3"
  }
}
